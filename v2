if not _G.ExecutedBall then do
wait(0.1)
_G.ExecutedBall = true

local grabball = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local CheckBoxes = Instance.new("Folder")
local CheckBoxAnimated = Instance.new("Frame")
local UIPadding = Instance.new("UIPadding")
local Frame_2 = Instance.new("Frame")
local TextButton = Instance.new("TextButton")
local Val = Instance.new("Frame")
local TextLabel = Instance.new("TextLabel")
local UICorner = Instance.new("UICorner")

--Properties:

grabball.Name = "grab ball"
grabball.ResetOnRespawn = false
grabball.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
grabball.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = grabball
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
